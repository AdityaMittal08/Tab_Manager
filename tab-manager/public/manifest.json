{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0.0",
  "description": "A simple and elegant way to organize, search, and save your browser tabs.",
  "icons": {
    "16": "leaf16.png",
    "48": "leaf48.png",
    "128": "leaf128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "leaf16.png",
      "32": "leaf32.png"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "service_worker": "service-worker.js"
  }
}